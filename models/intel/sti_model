MODEL x86

typedef exitMode : enum {gp, normal};

CONST
(*Control Register 0 variables*)
PE : BITVEC[1];
VME : BITVEC[1];

(*Control Register 4 variables*)
PVI : BITVEC[1];


cpl : BITVEC[2];
(*0-16bit, 1-32bit, >1-64bit*)
OPSIZE : BITVEC[2];

64bitmode : BITVEC[1];

RFLAGS_I : BITVEC[64];




